<?xml version="1.0"?>

<sdf version='1.6'>
<!-- Basic model of the 16' WAM-V USV -->
<model name="wamv2">
  <static>false</static>
  <link name="base_link">
    <enable_wind>true</enable_wind>
    <inertial>
      <pose>0 0 -0.2 0 0 0</pose>
      <mass>10</mass>
      <inertia>
        <ixx>0.5666666666666668</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>1.0416666666666667</iyy>
        <iyz>0</iyz>
        <izz>1.5416666666666667</izz>
      </inertia>
    </inertial>

    <visual name="visual">
      <pose>-0.3 -6.9 -0.15 0 0 1.57079632679</pose>
      <geometry>
        <mesh><uri>meshes/warship.dae</uri></mesh>
      </geometry>
    </visual>

    <collision name="collision">
      <pose>-0.3 -6.9 -0.15 0 0 1.57079632679</pose>
      <geometry>
        <mesh>
          <uri>meshes/warship.dae</uri>
          <scale>1 1 1</scale>
        </mesh>
      </geometry>
    </collision>
  </link>

  <!-- left engine and thruster -->
  <link name="left_housing_link">
    <inertial>
      <pose>-0.3 0.33 -0.15 0 0 0</pose>
      <mass>1</mass>
      <inertia>
        <ixx>0.0020290833333333333</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>0.002408333333333333</iyy>
        <iyz>0</iyz>
        <izz>0.0006874166666666666</izz>
      </inertia>
    </inertial>
    <visual name="left_housing_visual">
      <pose>-0.3 0.33 -0.15 0 0 0</pose>
      <geometry>
        <mesh>
          <uri>meshes/housing.dae</uri>
        </mesh>
      </geometry>
      <material>
        <diffuse>1 1 1</diffuse>
        <specular>1 1 1</specular>
        <pbr>
          <metal>
            <albedo_map>materials/textures/Thruster_Albedo.png</albedo_map>
            <roughness_map>materials/textures/Thruster_Roughness.png</roughness_map>
          </metal>
        </pbr>
      </material>
    </visual>
    <visual name="left_mount_visual">
      <pose>-0.3 0.33 -0.15 0 0 0</pose>
      <geometry>
        <mesh>
          <uri>meshes/mount.dae</uri>
        </mesh>
      </geometry>
      <material>
        <diffuse>1 1 1</diffuse>
        <specular>1 1 1</specular>
        <pbr>
          <metal>
            <albedo_map>materials/textures/Thruster_Albedo.png</albedo_map>
            <roughness_map>materials/textures/Thruster_Roughness.png</roughness_map>
          </metal>
        </pbr>
      </material>
    </visual>
    <collision name="Box011d">
      <pose>-0.3 0.33 -0.049049 0 0 0</pose>
      <geometry>
        <box>
          <size>0.0810453 0.0430519 0.070925</size>
        </box>
      </geometry>
    </collision>
    <collision name="Cylinder264d">
      <pose>-0.3 0.33 -0.0871934 0 0 0</pose>
      <geometry>
        <cylinder>
          <length>0.125613</length>
          <radius>0.014703</radius>
        </cylinder>
      </geometry>
    </collision>
    <collision name="Cylinder263d">
      <pose>-0.2833463 0.329867 -0.15 1.5708 0 -1.5708</pose>
      <geometry>
        <cylinder>
          <length>0.14376</length>
          <radius>0.0243692</radius>
        </cylinder>
      </geometry>
    </collision>
  </link>

  <link name="left_prop_link">
    <inertial>
      <pose>-0.300872875 0.329867016 -0.15 1.5708 0 -1.5708</pose>
      <mass>0.1</mass>
      <inertia>
        <ixx>0.000105625</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>0.000105625</iyy>
        <iyz>0</iyz>
        <izz>0.00015125</izz>
      </inertia>
    </inertial>
    <visual name="left_prop_visual">
      <pose>-0.3 0.33 -0.15 0 0 0</pose>
      <geometry>
        <mesh>
          <uri>meshes/prop.dae</uri>
        </mesh>
      </geometry>
      <material>
        <diffuse>1 1 1</diffuse>
        <specular>1 1 1</specular>
        <pbr>
          <metal>
            <albedo_map>materials/textures/Thruster_Albedo.png</albedo_map>
            <roughness_map>materials/textures/Thruster_Roughness.png</roughness_map>
          </metal>
        </pbr>
      </material>
    </visual>
    <collision name="Cylinder262d">
      <pose>-0.300872875 0.329867016 -0.15 1.5708 0 -1.5708</pose>
      <geometry>
        <cylinder>
          <length>0.0608172</length>
          <radius>0.0553844</radius>
        </cylinder>
      </geometry>
    </collision>
  </link>

  <joint name="left_chasis_engine_joint" type="revolute">
    <axis>
      <xyz>0 0 1</xyz>
      <limit>
        <lower>0</lower>
        <upper>0</upper>
        <effort>10</effort>
        <velocity>10</velocity>
      </limit>
    </axis>
    <parent>base_link</parent>
    <child>left_housing_link</child>
  </joint>
  
  <joint name="left_engine_propeller_joint" type="revolute">
    <pose>-0.300872875 0.329867016 -0.15 1.5708 0 0</pose>
    <axis>
      <xyz>1 0 0</xyz>
      <limit>
        <effort>50</effort>
        <velocity>50</velocity>
      </limit>
      <dynamics>
        <friction>0.05</friction>
        <damping>0.05</damping>
      </dynamics>
    </axis>
    <parent>left_housing_link</parent>
    <child>left_prop_link</child>
  </joint>

  <!-- right engine and thruster -->
  <link name="right_housing_link">
    <inertial>
      <pose>-0.3 -0.33 -0.15 0 0 0</pose>
      <mass>1</mass>
      <inertia>
        <ixx>0.0020290833333333333</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>0.002408333333333333</iyy>
        <iyz>0</iyz>
        <izz>0.0006874166666666666</izz>
      </inertia>
    </inertial>
    <visual name="right_housing_visual">
      <pose>-0.3 -0.33 -0.15 0 0 0</pose>
      <geometry>
        <mesh>
          <uri>meshes/housing.dae</uri>
        </mesh>
      </geometry>
      <material>
        <diffuse>1 1 1</diffuse>
        <specular>1 1 1</specular>
        <pbr>
          <metal>
            <albedo_map>materials/textures/Thruster_Albedo.png</albedo_map>
            <roughness_map>materials/textures/Thruster_Roughness.png</roughness_map>
          </metal>
        </pbr>
      </material>
    </visual>
    <visual name="right_mount_visual">
      <pose>-0.3 -0.33 -0.15 0 0 0</pose>
      <geometry>
        <mesh>
          <uri>meshes/mount.dae</uri>
        </mesh>
      </geometry>
      <material>
        <diffuse>1 1 1</diffuse>
        <specular>1 1 1</specular>
        <pbr>
          <metal>
            <albedo_map>materials/textures/Thruster_Albedo.png</albedo_map>
            <roughness_map>materials/textures/Thruster_Roughness.png</roughness_map>
          </metal>
        </pbr>
      </material>
    </visual>
    <collision name="Box011i">
      <pose>-0.3 -0.33 -0.049049 0 0 0</pose>
      <geometry>
        <box>
          <size>0.0810453 0.0430519 0.070925</size>
        </box>
      </geometry>
    </collision>
    <collision name="Cylinder264i">
      <pose>-0.3 -0.33 -0.0871934 0 0 0</pose>
      <geometry>
        <cylinder>
          <length>0.125613</length>
          <radius>0.014703</radius>
        </cylinder>
      </geometry>
    </collision>
    <collision name="Cylinder263i">
      <pose>-0.2833463 -0.329867 -0.15 1.5708 0 -1.5708</pose>
      <geometry>
        <cylinder>
          <length>0.14376</length>
          <radius>0.0243692</radius>
        </cylinder>
      </geometry>
    </collision>
  </link>
  
  <link name="right_prop_link">
    <inertial>
      <pose>-0.300872875 -0.329867016 -0.15 1.5708 0 -1.5708</pose>
      <mass>0.1</mass>
      <inertia>
        <ixx>0.000105625</ixx>
        <ixy>0</ixy>
        <ixz>0</ixz>
        <iyy>0.000105625</iyy>
        <iyz>0</iyz>
        <izz>0.00015125</izz>
      </inertia>
    </inertial>
    <visual name="right_prop_visual">
      <pose>-0.3 -0.33 -0.15 0 0 0</pose>
      <geometry>
        <mesh>
          <uri>meshes/prop.dae</uri>
        </mesh>
      </geometry>
      <material>
        <diffuse>1 1 1</diffuse>
        <specular>1 1 1</specular>
        <pbr>
          <metal>
            <albedo_map>materials/textures/Thruster_Albedo.png</albedo_map>
            <roughness_map>materials/textures/Thruster_Roughness.png</roughness_map>
          </metal>
        </pbr>
      </material>
    </visual>
    <collision name="Cylinder262i">
      <pose>-0.300872875 -0.329867016 -0.15 1.5708 0 -1.5708</pose>
      <geometry>
        <cylinder>
          <length>0.0608172</length>
          <radius>0.0553844</radius>
        </cylinder>
      </geometry>
    </collision>
  </link>

  <joint name="right_chasis_engine_joint" type="revolute">
    <axis>
      <xyz>0 0 1</xyz>
      <limit>
        <lower>0</lower>
        <upper>0</upper>
        <effort>10</effort>
        <velocity>10</velocity>
      </limit>
    </axis>
    <parent>base_link</parent>
    <child>right_housing_link</child>
  </joint>
  
  <joint name="right_engine_propeller_joint" type="revolute">
    <pose>-0.300872875 -0.329867016 -0.15 1.5708 0 0</pose>
    <axis>
      <xyz>1 0 0</xyz>
      <limit>
        <effort>50</effort>
        <velocity>50</velocity>
      </limit>
      <dynamics>
        <friction>0.05</friction>
        <damping>0.05</damping>
      </dynamics>
    </axis>
    <parent>right_housing_link</parent>
    <child>right_prop_link</child>
  </joint>

  <!-- Uncomment to enable the left thruster -->
  <plugin
    filename="gz-sim-thruster-system"
    name="gz::sim::systems::Thruster">
    <joint_name>left_engine_propeller_joint</joint_name>
    <thrust_coefficient>1</thrust_coefficient>
    <fluid_density>1000</fluid_density>
    <max_thrust_cmd>2.5</max_thrust_cmd>
    <min_thrust_cmd>-2.5</min_thrust_cmd>
    <propeller_diameter>0.1</propeller_diameter>
    <velocity_control>true</velocity_control>
  </plugin>

  <plugin
    filename="gz-sim-joint-position-controller-system"
    name="gz::sim::systems::JointPositionController">
    <joint_name>left_chasis_engine_joint</joint_name>
    <use_velocity_commands>true</use_velocity_commands>
    <topic>/wamv/left/thruster/joint/cmd_pos</topic>
  </plugin>

  <!-- Uncomment to enable the right thruster -->
  <plugin
    filename="gz-sim-thruster-system"
    name="gz::sim::systems::Thruster">
    <joint_name>right_engine_propeller_joint</joint_name>
    <thrust_coefficient>1</thrust_coefficient>
    <fluid_density>1000</fluid_density>
    <max_thrust_cmd>2.5</max_thrust_cmd>
    <min_thrust_cmd>-2.5</min_thrust_cmd>
    <propeller_diameter>0.1</propeller_diameter>
    <velocity_control>true</velocity_control>
  </plugin>

  <plugin
    filename="gz-sim-joint-position-controller-system"
    name="gz::sim::systems::JointPositionController">
    <joint_name>right_chasis_engine_joint</joint_name>
    <use_velocity_commands>true</use_velocity_commands>
    <topic>/wamv/right/thruster/joint/cmd_pos</topic>
  </plugin>
  <!-- Uncomment to produce buoyancy on the left hull -->
  <plugin filename="libSurface.so" name="maritime::Surface">
    <link_name>base_link</link_name>
    <hull_length>1.15</hull_length>
    <hull_radius>0.15</hull_radius>
    <fluid_level>0.8</fluid_level>
    <points>
      <point>0.68 0.314338 0.0352701</point>
      <point>-0.465 0.314338 0.0352701</point>
    </points>
    <wavefield>
      <topic>/maritime/wavefield/parameters</topic>
    </wavefield>
  </plugin>
  
  <plugin filename="libSurface.so" name="maritime::Surface">
    <link_name>base_link</link_name>
    <hull_length>1.15</hull_length>
    <hull_radius>0.15</hull_radius>
    <fluid_level>0.8</fluid_level>
    <points>
      <point>0.68 -0.314338 0.0352701</point>
      <point>-0.465 -0.314338 0.0352701</point>
    </points>
    <wavefield>
      <topic>/maritime/wavefield/parameters</topic>
    </wavefield>
  </plugin>
  <!-- Uncomment to enable hydrodynamics -->
  <plugin
    filename="gz-sim-hydrodynamics-system"
    name="gz::sim::systems::Hydrodynamics">
    <link_name>base_link</link_name>
      <xDotU>0.0</xDotU>
      <yDotV>0.0</yDotV>
      <nDotR>0.0</nDotR>
      <xU>-4.0</xU>
      <xAbsU>6.0</xAbsU>
      <yV>-4.0</yV>
      <yAbsV>4.0</yAbsV>
      <zW>-20.0</zW>
      <kP>-12.0</kP>
      <mQ>-36.0</mQ>
      <nR>-32.0</nR>
      <nAbsR>32.0</nAbsR>
  </plugin>


  <plugin
		 filename="gz-sim-joint-controller-system"
		 name="gz::sim::systems::JointController">
		 <joint_name>j1</joint_name>
		 <initial_velocity>0</initial_velocity>
  </plugin>

  <plugin
		 filename="gz-sim-joint-controller-system"
		 name="gz::sim::systems::JointController">
		 <joint_name>j2</joint_name>
		 <initial_velocity>0</initial_velocity>
	</plugin>
  <link name="gr">
    <pose>1.1 0 0.1 0 0 0</pose>
    <collision name="collision">
    <geometry>
      <mesh>
        <uri>meshes/gr.dae</uri>
        <scale>0.05 0.05 0.05</scale>
      </mesh>
    </geometry>
  </collision>
  <visual name="visual">
    <geometry>
      <mesh>
        <uri>meshes/gr.dae</uri>
        <scale>0.05 0.05 0.05</scale>
      </mesh>
    </geometry>
  </visual>
  </link>
  <link name="gun">
    <pose>1.1 0 0.32 0.0 0 0</pose>
    <collision name="collision">
    <geometry>
      <mesh>
        <uri>meshes/gun.dae</uri>
        <scale>0.05 0.05 0.05</scale>
      </mesh>
    </geometry>
  </collision>
  <visual name="visual">
    <geometry>
      <mesh>
        <uri>meshes/gun.dae</uri>
        <scale>0.05 0.05 0.05</scale>
      </mesh>
    </geometry>
  </visual>
  </link>
  <joint name="j2" type="revolute">
    <parent>base_link</parent>
    <child>gr</child>
    <axis>
      <xyz>0 0 1</xyz>
      <limit>
      <effort>10</effort>
      <velocity>1.0</velocity>
      </limit>
    </axis>
  </joint>
  <joint name="j1" type="revolute">
    <pose>0 0 0 0 0 0</pose>
    <parent>gr</parent>
    <child>gun</child>
    <axis>
      <xyz>0 1 0</xyz>
      <limit>
      <lower>-1.57</lower>
      <upper>1.57 </upper>
      <effort>10</effort>
      <velocity>1.0</velocity>
      </limit>
    </axis>
  </joint>
</model>

</sdf>
